                                        <li><a href="#">One more separated link</a></li>*@
                                </ul>
                            </li>
                        </ul>
                        <div id="loginOptions">@Html.Partial("_LoginPartial")</div>
                    </div>
                </div>
            </div>

        </div>
    </div>

    @RenderBody()

